{
	"name": "acme-api-dev",
	"version": "2.0.10",
	"private": true,
	"description": "Acme Office API (Development)",
	"keywords": [
		"api-builder",
		"micro-service",
		"middleware",
		"orm",
		"service"
	],
	"repository": {},
	"license": "",
	"main": "app.js",
	"scripts": {
		"copy:source": "node ./scripts/internal/distribution/copy-source.js",
		"docker:build": "docker build --tag $npm_package_name ./",
		"fix:port": "acs config --set PORT=8080  $npm_package_name",
		"fix:size": "acs server --set Medium  $npm_package_name",
		"lint": "eslint . --fix",
		"logcat": "acs logcat $npm_package_name",
		"pub:dbdocs": "dbdocs build ./design/database/project.dbml",
		"pub:dev": "npm run switch:dev && npm run docker:build && npm run pub:docker",
		"pub:docker": "acs publish $npm_package_name --delete_oldest --force --image $npm_package_name --app_version $npm_package_version",
		"pub:prod": "npm run switch:prod && npm run docker:build && npm run pub:docker",
		"start": "node .",
		"switch": "node ./scripts/internal/branding/switch-environment.js",
		"postswitch": "npm install",
		"switch:acme": "npm run switch acme",
		"switch:dev": "npm run switch dev",
		"switch:local": "npm run switch local",
		"switch:prod": "npm run switch prod",
		"test": "mocha --colors ./test --recursive -R spec"
	},
	"dependencies": {
		"@axway/api-builder-plugin-fn-base64": "^2.0.0",
		"@axway/api-builder-plugin-fn-javascript": "^1.0.0",
		"@axway/api-builder-plugin-fn-json": "^2.0.0",
		"@axway/api-builder-plugin-fn-mustache": "^1.0.1",
		"@axway/api-builder-plugin-fn-restclient": "^2.0.0",
		"@axway/api-builder-plugin-fn-swagger": "^2.0.0",
		"@axway/api-builder-runtime": "^4.22.0",
		"@geek/dotenv": "0.0.9",
		"@geek/google-sheets": "0.0.6",
		"@geek/jwt": "1.0.1",
		"@titanium/please": "1.0.10",
		"bluebird": "^3.7.2",
		"dotenv": "^8.2.0",
		"fs-extra": "^9.0.1",
		"handlebars": "^4.7.6",
		"json-schema-ref-parser": "^9.0.6",
		"jsonc-parser": "^2.3.1",
		"lodash": "^4.17.20",
		"moment": "^2.29.1",
		"parse": "^2.17.0",
		"safe-json-stringify": "^1.2.0",
		"socket.io": "^2.3.0",
		"sugar": "^2.0.6",
		"twitter-lite": "^0.14.0",
		"winston": "^3.3.3",
		"winston-logzio": "^5.1.2"
	},
	"devDependencies": {
		"@axway/api-builder-admin": "^1.10.10",
		"@geek/eslint-config": "0.0.17",
		"@geek/jsonc": "1.0.0",
		"chai": "^4.2.0",
		"cpy": "^8.1.1",
		"dbdocs": "^0.3.1",
		"del": "^6.0.0",
		"eslint": "^7.12.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jsdoc": "^30.7.3",
		"eslint-plugin-promise": "^4.2.1",
		"mocha": "^8.2.0"
	},
	"engines": {
		"_node": "12.16.3",
		"node": "14.3.1"
	}
}
